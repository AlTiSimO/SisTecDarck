		  ],
		  "settings": {
			"background": "#272822",
			"foreground": "#F8F8F2"
		  }
		},
		{
		  "name": "Comment",
		  "scope": "comment",
		  "settings": {
			"foreground": "#75715E"
		  }
		},
		{
		  "name": "String",
		  "scope": "string",
		  "settings": {
			"foreground": "#E6DB74"
		  }
		},
		{
		  "name": "Template Definition",
		  "scope": [
			"punctuation.definition.template-expression",
			"punctuation.section.embedded"
		  ],
		  "settings": {
			"foreground": "#F92672"
		  }
		},
		{
		  "name": "Reset JavaScript string interpolation expression",
